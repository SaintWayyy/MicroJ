class Classroom {
    static int[] ids;
    private static int[] scores;
    constructor Classroom(){}

    void init_ids(Classroom self){
        Classroom.ids = new int[5];
        for (int i : = 0; i < 5; i = i + 1) {
            Classroom.ids[i] : = i;
            print(Classroom.ids[i]);
        }
    }

    void init_scores(Classroom self){
        Classroom.scores = new int[10];
        for (int i : = 0; i < 10; i = i + 1) {
            Classroom.scores[i] : = i;
            print(Classroom.scores[i]);
        }
    }
}

int main() {
    Classroom a : = new Classroom();
    a.init_ids();
    a.init_scores();

}